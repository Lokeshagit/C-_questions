#include <bits/stdc++.h>
using namespace std;
#define ll long long
long long power(long long base, long long exp) {
    long long result = 1;
    while (exp > 0) {
        if (exp % 2 == 1) {
            result *= base;
        }
        base *= base;
        exp /= 2;
    }
    return result;
}
void solve() {
    int n;
    cin >> n;
    string s, q;
    cin >> s >> q;
    for (int i = 0; i < n; i++) {
        if (s[i] == q[i]) {
            s[i] = '0';
        } else {
            s[i] = '1';
        }
    }
    cout<<s<<endl;
    ll ans = 0;
    for (ll i = 0; i < n; i++) {
        for (ll j = i; j < n; j++) {
            string sub = s.substr(i, j - i + 1);
            ll x = stoll(sub, nullptr, 2);
            ans = max(ans, (j - i + 1) / power(2,x));
        }
    }
    cout << ans << endl;
}
 
 
 
int main() {
    int t;
    cin >> t;
    while (t--) {
        solve();
    }
}
